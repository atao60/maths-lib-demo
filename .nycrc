{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    ".yarn/**",
    "**/*.d.ts",
    "**/*.test.ts",
    ".eslintrc.js",
    ".mocharc.js",
    ".pnp.js",
    "commitlint.config.js"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}
